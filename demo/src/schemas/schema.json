{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "todo",
        "completed",
        "userId"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                3
            ]
        },
        "todo": {
            "type": "string",
            "default": "",
            "title": "The todo Schema",
            "examples": [
                "Watch a classic movie"
            ]
        },
        "completed": {
            "type": "boolean",
            "default": false,
            "title": "The completed Schema",
            "examples": [
                false
            ]
        },
        "userId": {
            "type": "integer",
            "default": 0,
            "title": "The userId Schema",
            "examples": [
                4
            ]
        }
    },
    "examples": [{
        "id": 3,
        "todo": "Watch a classic movie",
        "completed": false,
        "userId": 4
    }]
}